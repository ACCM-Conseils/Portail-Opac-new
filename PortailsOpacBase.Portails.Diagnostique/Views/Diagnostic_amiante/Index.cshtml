                                                $('#div_logements_controles').html(result);

                                                $('#divlogementsFichiersnew').html(result);

                                                $('#divlogementsFichiersnew .lgt_diag').click(function (e) {
                                                    var prop = $(this);
                                                    if ($('#divlogementsFichiersnew .lgt_diag:checked').length > 0) {
                                                        $('#divlogementsFichiersnew .lgt_diag:checked').prop("checked", false);
                                                        prop.prop("checked", true);
                                                    }
                                                });

                                                if ($('#div_logements_controles tbody tr').length > 0)
                                                    $('#modifierinfos').prop("disabled", true);
                                                else
                                                    $('#modifierinfos').prop("disabled", false);

                                                if ($('#div_logements_controles .lgt_diag').length > 0) {
                                                    $('#addcontrole').prop("disabled", false);
                                                    $('#addfiles').prop("disabled", false);
                                                }

                                                $('#div_logements_controles .lgt_diag').change(function (e) {
                                                    if ($('#div_logements_controles .lgt_diag:checked').length > 0) {
                                                        $('#dellogement').prop("disabled", false);
                                                    }
                                                    else
                                                        $('#dellogement').prop("disabled", true);

                                                    if ($('#typefile').val().length > 0 && $('#divfichiers .file').length > 0 && $('#numrapport').val().length > 0) {
                                                        $('#validfichiers').prop("disabled", false);
                                                    }
                                                    else {
                                                        $('#validfichiers').prop("disabled", true);
                                                    }
                                                });

                                                $('#divlogementsFichiers .lgt_diag').change(function (e) {

                                                    if ($('#typefile').val().length > 0 && $('#divfichiers .file').length > 0 && $('#numrapport').val().length > 0) {
                                                        $('#validfichiers').prop("disabled", false);
                                                    }
                                                    else {
                                                        $('#validfichiers').prop("disabled", true);
                                                    }
                                                });
                                            }
                                    });
                                    $.ajax({
                                        url: "@Html.Raw(Url.Action("ListeDiagEffectues", "Diagnostic_amiante"))",
                                            cache: false,
                                            type: "get",
                                            dataType: "html",
                                        success: function (result) {
                                            $('#div_diag_controles').html(result);
                                            $('.seldiag').change(function (e) {
                                                if ($(this).prop("checked")) {
                                                    $('.seldiag').prop("checked", false);
                                                    $(this).prop("checked", true);
                                                    $('#modcontrole').prop("disabled", false);
                                                    $('#delcontrole').prop("disabled", false);
                                                }
                                                else {
                                                    $('#modcontrole').prop("disabled", true);
                                                    $('#delcontrole').prop("disabled", true);
                                                }
                                            });
                                        }
                                    });
                                    $.ajax({
                                        url: "@Html.Raw(Url.Action("Fichiers", "Diagnostic_amiante"))",
                                            cache: false,
                                            type: "get",
                                            dataType: "html",
                                        success: function (result) {
                                            $('#div_diag_fichiers').html(result);
                                            $('.delfichierlogement').click(function (e) {
                                                DelFile($(this).data("id"), $(this).data("ajout"));
                                            });
                                            $('.selectfilelogement').click(function (e) {
                                                $('#gbalfichier').html($(this).data("gbal"));
                                                $('#idlogementfichier').val($(this).data("id"));
                                                $('#dialog-fichiers').modal("show");
                                            });
                                        }
                                    });
                                    $.ajax({
                                        url: "@Html.Raw(Url.Action("GetFichiers", "Diagnostic_amiante"))",
                                        data: { typelogement: $("input[name='choix']:checked").val() },
                                        cache: false,
                                        type: "get",
                                        dataType: "html",
                                        beforeSend: function (e) {
                                            $('#divtypefichiers').html("");
                                            $('#divtypefichiersnew').html("");
                                        },
                                        success: function (result) {
                                            $('#divtypefichiers').html(result);
                                            $('#divtypefichiersnew').html(result);

                                            $('#typefile').show();

                                            $('#typefile').change(function (e) {
                                                if ($(this).val().length > 0 && $('#divfichiers .file').length > 0 && $('#numrapport').val().length > 0) {
                                                    $('#validfichiers').prop("disabled", false);
                                                }
                                                else {
                                                    $('#validfichiers').prop("disabled", true);
                                                }
                                            });
                                        }
                                    });
                                    $.ajax({
                                        url: "@Html.Raw(Url.Action("LogementsAControler", "Diagnostic_amiante"))",
                                            cache: false,
                                            type: "get",
                                            dataType: "html",
                                            success: function (result) {
                                                $('#divlogementsacontroler').html(result);                                                

                                                if ($('#div_logements_controles tr').length > 0)
                                                    $('#modifierinfos').prop("disabled", true);
                                            }
                                        });
                                }
                                else {
                                    alert("Groupe/Batiment/Allée inexistant");
                                }
                            }
                        });
                    }
                    }
                });
        }
        $.ajax({
            url: "@Html.Raw(Url.Action("listeLogements", "Diagnostic_amiante"))",
